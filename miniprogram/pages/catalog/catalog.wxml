<block wx:for="{{arr}}">
    <!-- <view >{{item}}</view> -->
    <view >{{index}}:{{item.count}}</view>
    <view >{{index}}:{{item.evaluation}}</view>
</block>

<form bindsubmit="formsubmit" bindreset="formreset">

    <view class="section">
      <view class="section__title">你输入的是：{{inputValue}}</view>
      <input name="input" bindinput="bindKeyInput" placeholder="输入同步到view中" />
    </view>

    <slider name="slider" show-value >选择你的分数</slider>

    <button formType="submit">搜索</button>
    <button formType="reset">清空</button>
</form>

<view class="page">
    <view class="page__hd">
        <view class="page__title">课程分类</view>
        <view class="page__desc">按类别检索选修课</view>
    </view>
    <view class="page__bd page__bd_spacing">
        <button class="weui-btn" type="default" bindtap="jumpPage1">校公选课</button>
        <button class="weui-btn" type="default" bindtap="jumpPage2">拓展英语</button>
        <button class="weui-btn" type="default" bindtap="jumpPage3">科技实践</button>
        <button class="weui-btn" type="default" bindtap="jumpPage4">艺术实践</button>
        <button class="weui-btn" type="default" bindtap="jumpPage5">文化实践</button>
        <button class="weui-btn" type="default" bindtap="jumpPage6">体育选课</button>
    </view>
</view>

